package rules.tradeManagement;

import org.trading.model.*;
import org.trading.event.*;
import org.trading.ChannelIds;

dialect  "mvel"

rule "On each OpeningRange"
    when
      $or: OpeningRange($e: epic) from entry-point "OPENING_RANGE"
    then
      System.out.println("Insert opening range for " + $e);
      insert($or);
end